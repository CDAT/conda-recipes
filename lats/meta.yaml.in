package:
    name: lats
    version: 1.0.1

source:
    git_rev: master
    git_url: git://github.com/cdat/lats.git

build:
  number: @BUILD_NUMBER@
 
requirements:
  build:
    - libnetcdf 4.*
    - hdf5 1.10.*
    - gcc_linux-64 [linux]
    - gfortran_linux-64 [linux]
  run:
    - libnetcdf 4.*
    - hdf5 1.10.*
    - libgcc [linux]
    - libgfortran [linux]

about:
    home:  http://github.com/cdat/lats



