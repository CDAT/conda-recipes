package:
    name: vcs
    version: @VERSION@.{{ GIT_FULL_HASH }}

source:
    git_rev: @UVCDAT_BRANCH@
    git_url: git://github.com/UV-CDAT/vcs.git

build:
  number: @BUILD_NUMBER@
 
requirements:
  build:
    - python <3
    - setuptools
    - cdat_info
  run:
    - python <3
    - cdat_info
    - genutil
    - unidata
    - dv3d
    - vtk-cdat
    - cdms2
    - cdutil
    - ffmpeg
    - vcsaddons
    - requests
    - numpy

about:
    home:  http://gitub.com/UV-CDAT/uvcdat



