package:
    name: vcs
    version: @VERSION@

source:
    git_rev: @UVCDAT_BRANCH@
    git_url: git://github.com/UV-CDAT/vcs.git

build:
  number: @BUILD_NUMBER@
 
requirements:
  build:
    - python <3
    - setuptools
    - cdat_info
  run:
    - python <3
    - cdat_info
    - genutil
    - dv3d
    - vtk-cdat >7.1.0.2.8
    - cdms2
    - cdutil
    - requests
    - numpy

about:
    home:  http://gitub.com/UV-CDAT/uvcdat



