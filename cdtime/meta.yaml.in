package:
    name: cdtime
    version: @VERSION@

source:
    git_rev: @UVCDAT_BRANCH@
    git_url: git://github.com/UV-CDAT/cdtime.git

build:
  number: @BUILD_NUMBER@
 
requirements:
  build:
    - python {{ python }}
    - setuptools
    - libcdms
    - numpy {{ numpy }}
    - cdat_info
    - libdrs_f
  run:
    - python
    - setuptools
    - libcdms
    - cdat_info
    - libdrs_f
    - numpy 

about:
    home:  http://gitub.com/UV-CDAT/cdtime



