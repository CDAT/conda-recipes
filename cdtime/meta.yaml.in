package:
    name: cdtime
    version: @VERSION@

source:
    git_rev: @UVCDAT_BRANCH@
    git_url: git://github.com/UV-CDAT/cdtime.git

build:
  number: @BUILD_NUMBER@
 
requirements:
  build:
    - python {{ PY_VER }}
    - setuptools
    - libcdms
    - numpy  {{ NPY_VER }}
    - cdat_info
    - libdrs_f
    - toolchain
    - libgfortran
  run:
    - python {{ PY_VER }}
    - setuptools
    - libcdms
    - cdat_info
    - libdrs_f
    - libgfortran
    - numpy  {{ NPY_VER }}

about:
    home:  http://gitub.com/UV-CDAT/cdtime

