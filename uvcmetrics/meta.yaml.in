package:
    name: uvcmetrics
    version: @VERSION@

source:
    git_rev: @UVCDAT_BRANCH@
    git_url: git://github.com/UV-CDAT/uvcmetrics.git

build:
  string: UVCDAT
 
requirements:
  build:
    - python
    - setuptools
    - numpy <=1.9.2
  run:
    - python
    - numpy <=1.9.2
    - vcs
    - cdms2 >2.4.1
    - eztemplate
    - output_viewer >=1.1.0

about:
    home:  http://gitub.com/UV-CDAT/uvcmetrics
    summary: A set of diagnostics to analyze climate model simulations



