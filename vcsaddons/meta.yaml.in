package:
    name: vcsaddons
    version: "@VERSION@"

source:
    git_rev: @UVCDAT_BRANCH@
    git_url: git://github.com/cdat/vcsaddons.git

build:
  number: @BUILD_NUMBER@
 
requirements:
  build:
    - python
    - setuptools
    - toolchain  # [osx]
  run:
    - python
    - vcs
    - cdms2

about:
    home:  http://github.com/cdat/vcsaddons
    summary: Addons to VCS.



