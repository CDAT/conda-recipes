package:
    name: cdms2
    version: @VERSION@

source:
    git_rev: @UVCDAT_BRANCH@
    git_url: git://github.com/CDAT/cdms.git

build:
  number: @BUILD_NUMBER@
 
requirements:
  build:
    - {{ compiler('c') }}
  host:
    - python {{ python }}
    - setuptools
    - libcdms
    - libpng
    - cdat_info
    - numpy 
    - esmf >=7
    - libdrs_f
  run:
    - python {{ python }}
    - libcf
    - distarray
    - cdtime
    - libcdms
    - libpng
    - cdat_info
    - numpy 
    - esmf >=7
    - esmpy
    - libdrs_f
    - pyopenssl

about:
    home:  http://gitub.com/CDAT/cdms



