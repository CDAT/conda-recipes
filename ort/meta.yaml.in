package:
    name: ort
    version: @VERSION@

source:
    git_rev: @UVCDAT_BRANCH@
    git_url: git://github.com/UV-CDAT/uvcdat.git

build:
  string: @BUILD_NAME@
  detect_binary_files_with_prefix: true

test:
  requires:
    - python
    - gfortran [osx]
  imports:
    - ort

requirements:
  build:
    - python
    - setuptools
    - numpy <=1.9.2
    - gfortran [osx]
  run:
    - python
    - numpy <=1.9.2
    - gfortran [osx]

about:
    home:  http://gitub.com/UV-CDAT/uvcdat




