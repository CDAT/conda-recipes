package:
    name: ort
    version: @VERSION@

source:
    git_rev: @UVCDAT_BRANCH@
    git_url: git://github.com/UV-CDAT/uvcdat.git

build:
  number: @BUILD_NUMBER@
  detect_binary_files_with_prefix: true

test:
  requires:
    - python
    - gcc [osx]
  imports:
    - ort

requirements:
  build:
    - python
    - setuptools
    - numpy x.x
    - gcc [osx]
    - toolchain [osx]
  run:
    - python
    - numpy x.x
    - libgfortran [osx]

about:
    home:  http://gitub.com/UV-CDAT/uvcdat




