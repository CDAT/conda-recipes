package:
    name: ezget
    version: 1.0.1

source:
    git_rev: master
    git_url: git://github.com/cdat/ezget.git

build:
  number: @BUILD_NUMBER@
 
requirements:
  build:
    - libcdms
    - libdrs_f
    - gcc_linux-64 [linux]
    - gfortran_linux-64 [linux]
  run:
    - libcdms
    - libdrs_f
    - libgcc [linux]
    - libgfortran [linux]

about:
    home:  http://github.com/cdat/ezget



