package:
    name: genutil
    version: @VERSION@.npy{{ numpy }}

source:
    git_rev: @UVCDAT_BRANCH@
    git_url: git://github.com/CDAT/genutil.git

build:
  number: @BUILD_NUMBER@
 
requirements:
  build:
    - python {{ python }}
    - setuptools
    - numpy {{ numpy }}.*
    - udunits2
    - toolchain [osx]
  run:
    - python {{ python }}
    - cdms2
    - numpy {{ numpy }}.*
    - udunits2

about:
    home:  http://gitub.com/CDAT/genutil



