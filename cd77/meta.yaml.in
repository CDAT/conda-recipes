    git_url: git://github.com/cdat/cd77.git

build:
  number: @BUILD_NUMBER@
 
requirements:
  build:
    - python
    - setuptools
    - gcc_linux-64 [linux]
  run:
    - libcdms
    - libdrs_f
    - ezget
    - lats
    - python
    - libgcc [linux]

about:
    home:  http://github.com/cdat/cd77



