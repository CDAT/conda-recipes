package:
    name: libdrs_f
    version: 1.0.1

source:
    git_rev: new_gcc
    git_url: git://github.com/UV-CDAT/libdrs.git

build:
  number: @BUILD_NUMBER@
 
requirements:
  build:
    - g2clib
    - libcdms
    - libpng
    - gcc [osx]
    - gcc_linux-64 [linux]
    - gfortran_linux-64 [linux]
  run:
    - g2clib
    - libcdms
    - libpng
    - gcc [osx]
    - libgcc [linux]
    - libgfortran [linux]

about:
    home:  http://github.com/UV-CDAT/libdrs



